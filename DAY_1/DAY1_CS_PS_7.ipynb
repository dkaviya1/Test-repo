        "7.WRITE A PYTHON PROGRAM TO SUM THE FIRST 10 PRIME NUMBERS"
      ],
      "metadata": {
        "id": "bmC5b5kzSwnu"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "r=40\n",
        "for a in range(2,r+1):\n",
        "    c=0\n",
        "    for i in range(2,a):\n",
        "        if(a%i==0):\n",
        "         c=c+1\n",
        "    if(c<=0):\n",
        "        print(a,end=\" \")"
