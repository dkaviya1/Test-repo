      "source": [
        "1.WRITE A PYTHON FUNCTION TO PRINT EVEN AND ODD NUMBERS IN A LIST"
      ],
      "metadata": {
        "id": "dSI0EqDUbG6r"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "List = [1,2,3,4,5,6,7,8,9]\n",
        "\n",
        "even = []\n",
        "odd = []\n",
        "\n",
        "for i in List:\n",
        "    if(i % 2 == 0):\n",
        "        even.append(i)\n",
        "    else:\n",
        "        odd.append(i)\n",
        "\n",
        "print(\"Even number: \",even)\n",
        "print(\"Odd number: \",odd)"
      ],
      "metadata": {
