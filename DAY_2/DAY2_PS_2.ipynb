        "2.EVEN AND ODD NUMBERS"
      ],
      "metadata": {
        "id": "BMyuyICv9QKe"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "a= list(map(int, input(\"Enter numbers: \").split()))\n",
        "\n",
        "even_count =0\n",
        "odd_count = 0\n",
        "for num in a:\n",
        "    if num % 2 == 0:\n",
        "\n",
        "\n",
        "      even_count += 1\n",
        "\n",
        "    else:\n",
        "        odd_count += 1\n",
        "\n",
        "print(\"Even numbers in a: \", even_count)\n",
        "print(\"Odd numbers in a: \", odd_count)"
      ],
